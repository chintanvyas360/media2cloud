{
  "name": "videojs-markers-bundle",
  "version": "2.0.0",
  "description": "videojs-markers plugins for videojs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"bundle exists\" && exit 0",
    "build:clean": "rm -rf dist && mkdir -p dist",
    "build:download": "cd dist && wget https://codeload.github.com/spchuang/videojs-markers/zip/master -O videojs-markers.zip",
    "build:unpack": "cd dist && unzip videojs-markers.zip && cp -v videojs-markers*/dist/*.min.* .",
    "build": "npm-run-all -s build:clean build:download build:unpack"
  },
