{
  "name": "videojs-bundle",
  "version": "2.0.0",
  "description": "package videojs for browser",
  "main": "index.js",
  "scripts": {
    "test": "echo \"bundle exists\" && exit 0",
    "build:clean": "rm -rf dist && mkdir -p dist",
    "build:download": "cd dist && wget https://github.com/videojs/video.js/releases/download/v7.6.5/video-js-7.6.5.zip -O videojs.zip",
    "build:unpack": "cd dist && unzip videojs.zip",
    "build": "npm-run-all -s build:clean build:download build:unpack"
  },
