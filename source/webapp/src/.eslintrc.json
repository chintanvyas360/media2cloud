{
  "extends": "airbnb-base",
  "rules": {
    "strict": "error",
    "func-names": ["error", "never"],
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never"
    }],
    "no-use-before-define": ["error", {
      "functions": false,
      "classes": true,
      "variables": true
    }],
    "prefer-arrow-callback": 0,
    "prefer-object-spread": 0,
    "arrow-parens": 0,
    "max-classes-per-file": 0,
    "implicit-arrow-linebreak": 0,
    "operator-linebreak": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true
  },
  "globals": {
    "AWS": true,
    "AWSCognito": true,
    "AWSIoTData": true,
    "CryptoJS": true,
    "location": true,
    "AWSomeNamespace": true,
    "CardCollection": true,
    "VideoCard": true,
    "SystemMessageArea": true,
    "SettingsTabPanel": true,
    "MyCognito": true,
    "SignInModal": true,
    "FileUpload": true,
    "IotSubscriber": true,
    "FileUploader": true,
    "CustomVocabulary": true,
    "ConfigurationError": true,
    "SigV4Client": true,
    "SearchBox": true,
    "BasePreview": true,
    "VideoPreview": true,
    "ImagePreview": true,
    "Cropper": true,
    "SO0050": true,
    "AppUtils": true,
    "SparkMD5": true,
    "AmazonCognitoIdentity": true,
    "Storage": true,
    "ResetPasswordModal": true,
    "ImageEditor": true,
    "WorkteamWizard": true,
    "CardFactory": true,
    "BaseCard": true,
    "ImageCard": true,
    "ApiHelper": true,
    "mxReadable": true,
    "mxZero": true,
    "GoogleMap": true,
    "google": true,
    "JsonParser": true,
    "DefaultJsonParser": true,
    "CustomJsonParser": true,
    "BaseWizard": true,
    "MediaUploadWizard": true,
    "JsonUploadWizard": true,
    "ServiceAvailability": true,
    "videojs": true,
    "LabelEditor": true
  }
}
